{
  "include": [
    "src"
  ],
  "compilerOptions": {
      "target": "es5",
      "lib": [
        "esnext",
        "dom",
        "dom.iterable"
      ],
      "allowJs": true,
      "sourceMap": true,
      "noImplicitAny": false,
      "module": "esnext",
      "removeComments": true,
      "allowSyntheticDefaultImports": true,
      "jsx": "react",
      "baseUrl": "./src",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "moduleResolution": "node",
      "downlevelIteration": true,
      "types": [
        "@equinor/echo-scripts",
        "@types/webpack-env"
      ]
    }
}